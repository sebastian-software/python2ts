  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "python",
    "typescript",
    "itertools",
    "functools",
    "collections",
    "stdlib",
    "datetime",
    "regex",
    "range",
    "enumerate",
    "zip",
    "combinations",
    "permutations",
    "counter",
    "deque",
    "defaultdict",
    "lru-cache"
  ],
  "author": "Sebastian Software GmbH",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
