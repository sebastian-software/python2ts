{
  "name": "python2ts",
  "version": "1.4.3",
  "description": "AST-based Python to TypeScript transpiler. Convert Python code to clean, idiomatic TypeScript with full type preservation.",
  "homepage": "https://sebastian-software.github.io/python2ts/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastian-software/python2ts.git",
    "directory": "packages/python2ts"
  },
  "bugs": {
    "url": "https://github.com/sebastian-software/python2ts/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/sebastian-software"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "python2ts": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "python",
    "typescript",
    "transpiler",
    "compiler",
    "ast",
    "converter",
    "code-generation",
    "type-inference",
    "migration",
    "ai",
    "machine-learning"
  ],
  "author": "Sebastian Software GmbH",
  "license": "MIT",
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@lezer/common": "^1.5.0",
    "@lezer/python": "^1.1.18",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "pythonlib": "workspace:*",
    "typescript-eslint": "^8.53.1"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
